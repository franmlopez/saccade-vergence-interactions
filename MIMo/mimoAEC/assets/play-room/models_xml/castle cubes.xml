<!-- Castle -->

<body name="castle">
               
    <body name="red_cube" pos="-.1 -.2 .02">
        <geom class="scene" type="box" material="red" size="0.02 0.02 0.02" pos="0 0 0" density="200"/>
    </body>
    
    <body name="yellow_cube" pos=".05 .1 .02" euler="0 0 10">
        <geom class="scene" type="box" material="yellow" size="0.02 0.02 0.02" pos="0 0 0" density="200"/>
    </body>
    
    <body name="blue_cube" pos="-.05 -.05 .02" euler="0 0 15">
        <geom class="scene" type="box" material="blue" size="0.02 0.02 0.02" pos="0 0 0" density="200"/>
    </body>
    
    <body name="yellow_cylinder" pos=".4 .4 0.03">
        <geom class="scene" type="cylinder" material="yellow" size="0.0135 0.04" pos="0 0 0" density="200"/>
    </body>
    
    <body name="blue_cylinder" pos=".15 -.125 0.04" euler="90 40 0">
        <geom class="scene" type="cylinder" material="blue" size="0.0135 0.04" pos="0 0 0" density="200"/>
    </body>
    
    <body name="orange_sphere" pos=".2 0.1 0.029">
        <geom class="scene" type="sphere" material="orange" size="0.025" pos="0 0 0" density="200"/>
    </body>

    <body name="green_cylinder" pos=".2 .2 .04" euler="90 -20 0">
        <geom class="scene" type="cylinder" material="green" size="0.0135 0.04" density="200"/>
    </body>
    
</body>


